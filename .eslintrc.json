{
	"extends": "next/core-web-vitals",
	"plugins": ["@typescript-eslint"],
	"rules": {
		"quotes": ["error", "single"],
		"no-console": "warn",
		"no-unused-vars": "warn",
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"vars": "all",
				"args": "after-used",
				"ignoreRestSiblings": false
			}
		],
		"no-empty": "warn"
	}
}
