{
  "name": "@brandalley/smk",
  "version": "1.2.11",
  "description": "State Management Kit",
  "keywords": [],
  "license": "UNLICENSED",
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es5.js",
  "files": [
    "dist"
  ],
  "author": "BrandAlley PWA Frontend Team <developer@brandalley.co.uk>",
  "engines": {
    "node": ">=6.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@brandalley"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandalley/smk.git"
  },
  "scripts": {
    "create-index": "barrelsby -c barrelsby.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prebuild": "npm run create-index && rimraf dist",
    "build": "tsc --module commonjs --project tsconfig.build.json && rollup -c rollup.config.js",
    "start": "nodemon --ignore src/index.ts -e js,ts,tsx,json --watch src/ --exec yalc push --scripts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test --no-cache && npx depcheck",
    "depcheck": "depcheck",
    "prepack": "yarn prebuild && yarn build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run format",
      "eslint . --fix",
      "jest --bail â€”-findRelatedTests --passWithNoTests"
    ],
    "package.json": [
      "depcheck ."
    ]
  },
  "dependencies": {
    "i18n-js": "^3.8.0",
    "mobx": "^6.3.7",
    "mobx-state-tree": "^5.0.5",
    "reflect-metadata": "^0.1.13"
  },
  "yalcSig": "bbcc31ad1e955430757fac77a93fe0c0"
}
